{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "lib": ["es5", "es6", "dom"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false

  },
  "include": ["src/*"],
  "exclude": ["node_modules"]
}
